/*Задача 1. Разбиение числа
Напишите программу, которая находит количество способов разложить число n на сумму натуральных чисел.*/

#include <iostream>
#include <vector>
using namespace std;

// DP: количество разложений числа
int main() {
    int n;
    cin >> n;                    // Ввод n

    vector<vector<int>> dp(n+1, vector<int>(n+1, 0));
    // dp[i][j] — число способов разложить i, используя числа от 1..j

    for(int j = 0; j <= n; j++) dp[0][j] = 1;
    // 1 способ разложить 0 — пустым набором

    for(int i = 1; i <= n; i++)          // перебираем сумму
        for(int j = 1; j <= n; j++)      // перебираем доступные слагаемые
            if(j > i) dp[i][j] = dp[i][i];         // нельзя использовать j > i
            else dp[i][j] = dp[i][j-1] + dp[i-j][j];
            // либо не используем j, либо используем и уменьшаем число

    cout << dp[n][n];
}
