/*Задача 2: Самая длинная возрастающая подпоследовательность
Реализуйте функцию, которая находит длину самой длинной возрастающей подпоследовательности в массиве целых чисел.*/

#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<int> a;
    int x;
    while (cin >> x) a.push_back(x);  // читаем весь массив

    int n = a.size();
    vector<int> dp(n, 1);  // минимум длина = 1 (сам элемент)

    for(int i = 0; i < n; i++)
        for(int j = 0; j < i; j++)
            if(a[j] < a[i]) dp[i] = max(dp[i], dp[j] + 1);

    cout << *max_element(dp.begin(), dp.end());
}
